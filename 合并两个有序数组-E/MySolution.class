class MySolution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        int[] result = new int[m + n];
        if(m == 0) {
            for(int i = 0;i < nums2.length;i++) {
                nums1[i] = nums2[i];
            }
            Arrays.sort(nums1);
            return;
        }
        for(int i=0;i<nums1.length;i++) {
            if(m > i) {
                continue;
            }
            nums1[i] = nums2[i - m];
        }
        Arrays.sort(nums1);
    }
}